{
  "manifest_version": 1,
  "name": "Sorry United Not States of America",
  "version": "1.0",
  "description": "Removes instances of UAE or UK or United Arab Emirates or United Kingdom from most dropdown menus.",
  "author": "Taryn Young",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["SorryUnitedNotStatesOfAmerica.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ],
  "devtools_page": "devtools.html",
  "homepage_url": "http://www.github.com/YoungTaryn",
  "incognito": "spanning",
  "offline_enabled": true,
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  }
}
